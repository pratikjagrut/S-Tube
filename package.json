{
  "name": "sheng_li_video_blog",
  "version": "1.0.0",
  "description": "This is a local video blog such as YouTube conatins video on leadership by Mr. Steve.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=images/icons/mac/logo.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . S_TUBE --overwrite --asar=true --platform=win32 --arch=ia32 --icon=images/icons/win/logo.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=S_TUBE",
    "package-linux": "electron-packager . S_TUBE --overwrite --asar=true --platform=linux --arch=x64 --icon=images/icons/png/logo.png --prune=true --out=release-builds"
  },
  "author": "Pratik Jagrut",
  "license": "ISC",
  "devDependencies": {
    "electron": "1.7.10",
    "electron-packager": "^10.1.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "jquery": "^3.2.1",
    "mysql": "^2.15.0",
    "tether": "^1.4.3"
  }
}
